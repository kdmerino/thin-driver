// Driver Library definitions.
#include "driver.h"
#include <cstdio>

namespace dlib {
	/**
	 *  Constants.
	 */
	const char* LOAD = "load";
	const char* RUN = "run";
	const char* UNLOAD = "unload";

	/**
	 *  Prompts.
	 */
	const char* ERR_PROMPT = "No programs to %s...\n";
}


void dlib::load_programs() {
	printf(ERR_PROMPT, LOAD);
}

void dlib::run_programs() {
	printf(ERR_PROMPT, RUN);
}

void dlib::unload_programs() {
	printf(ERR_PROMPT, UNLOAD);
}

